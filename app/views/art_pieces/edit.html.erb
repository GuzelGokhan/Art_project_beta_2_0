<div class="container">

  <%= simple_form_for(@art_piece, url: art_piece_path , method: :patch) do |f| %>
  <h4> Edit Piece </h4>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :dimension %>
  <%= f.input :photo %>
  <%= f.submit "Add", class: 'btn btn-light' %>
  <%end%>

</div>